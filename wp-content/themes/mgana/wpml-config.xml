<wpml-config>
	<custom-types>
		<custom-type translate="1">la_team_member</custom-type>
		<custom-type translate="1">la_portfolio</custom-type>
	</custom-types>

	<custom-fields>
		<custom-field action="copy">_mgana_post_options</custom-field>
	</custom-fields>

	<taxonomies>
		<taxonomy translate="1">la_portfolio_category</taxonomy>
	</taxonomies>

	<admin-texts>
		<key name="mgana_options">

			<key name="breadcrumb_separator" />
			<key name="breadcrumb_translation_home" />
			<key name="breadcrumb_translation_error" />
			<key name="breadcrumb_translation_search" />

			<key name="header_mb_footer_bar_component">
				<key name="*">
					<key name="text"/>
					<key name="link"/>
					<key name="menu_id"/>
				</key>
			</key>

			<key name="related_product_title" />
			<key name="related_product_subtitle" />
			<key name="upsell_product_title" />
			<key name="upsell_product_subtitle" />
			<key name="thresholds_text1" />
			<key name="thresholds_text2" />
			<key name="crosssell_product_title" />
			<key name="crosssell_product_subtitle" />

			<key name="woo_custom_tabs">
				<key name="*">
					<key name="title"/>
					<key name="content"/>
				</key>
			</key>
			<key name="woo_custom_block_single_product">
				<key name="*">
					<key name="title"/>
					<key name="content"/>
				</key>
			</key>

			<key name="portfolio_custom_name" />
			<key name="portfolio_custom_name2" />
			<key name="portfolio_custom_slug" />
			<key name="portfolio_cat_custom_name" />
			<key name="portfolio_cat_custom_name2" />
			<key name="portfolio_cat_custom_slug" />

			<key name="404_page_content" />
			<key name="newsletter_popup_content" />
			<key name="popup_dont_show_text" />

			<key name="la_custom_blocks">
				<key name="*">
					<key name="title"/>
					<key name="content"/>
				</key>
			</key>

			<key name="thresholds_text1" />
			<key name="thresholds_text2" />

		</key>
	</admin-texts>
</wpml-config>